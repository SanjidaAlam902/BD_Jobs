<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bd_jobs.controllers.TicketSystemController">
   <children>
      <TextField fx:id="searchField" layoutX="40.0" layoutY="14.0" />
      <Button layoutX="285.0" layoutY="27.0" mnemonicParsing="false" onAction="#onSearch" text="Search" />
      <TableView fx:id="TicketTable" layoutX="65.0" layoutY="179.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="75.0" text="id" />
          <TableColumn fx:id="colTitle" prefWidth="75.0" text="title" />
        </columns>
      </TableView>
      <Button layoutX="403.0" layoutY="27.0" mnemonicParsing="false" onAction="#onNew" text="New" />
   </children>
</AnchorPane>
