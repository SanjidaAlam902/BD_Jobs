<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="282.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bd_jobs.LogInController">
   <children>
      <Label layoutX="151.0" layoutY="30.0" style="-fx-font-size: 18; -fx-font-weight: bold;" text="Sign in to your account" textFill="#2c59fa" />
      <Label fx:id="loginValidateLabel" layoutX="113.0" layoutY="72.0" text="Please enter your username and password to log in." />
      <TextField fx:id="usernameTextField" layoutX="129.0" layoutY="114.0" prefHeight="26.0" prefWidth="241.0" promptText="USERNAME" />
      <PasswordField fx:id="passwordTextField" layoutX="129.0" layoutY="158.0" prefHeight="26.0" prefWidth="242.0" promptText="PASSWORD" />
      <Button layoutX="227.0" layoutY="206.0" mnemonicParsing="false" onAction="#loginOnClick" text="Login" textFill="#0659d4" underline="true" />
      <Button layoutX="49.0" layoutY="222.0" mnemonicParsing="false" onAction="#OnClickNavigateCreateAccount" text="Sign Up" textFill="#17e53c" underline="true" wrapText="true" />
   </children>
</AnchorPane>
